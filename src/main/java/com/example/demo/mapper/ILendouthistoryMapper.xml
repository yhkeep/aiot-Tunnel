<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.example.demo.mapper.ILendouthistoryMapper"> 
    <!-- 插入维修 -->
    <insert id="insertLendoutHistory" parameterType="com.example.demo.domain.Lendouthistory">
        INSERT INTO lendouthistory (applaydepartment,handoverdepartment,applaypeople,successorpeople,handlingPeople,buttingPeople
        ,handoverimage,handovertime,estimatedtime,assetstatustype,remark,maintainhistoryonlyCode)
         VALUES (#{applaydepartment},#{handoverdepartment},#{applaypeople},#{successorpeople},#{handlingPeople},#{buttingPeople}
         ,#{handoverimage},#{handovertime},#{estimatedtime},#{assetstatustype},#{remark},#{maintainhistoryonlyCode})
    </insert>
    
    
</mapper>  